doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title emPHPasis Report
    // Font Awesome
    link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.0.8/css/all.css', integrity='sha384-3AB7yXWz4OeoZcPbieVW64vVXEwADiYyAEhwilzWsLw+9FgqpyjjStpPnpBO8o8S', crossorigin='anonymous')
    // Ionicons
    link(rel='stylesheet', href='http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css')
    // Google fonts
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,300', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Raleway', rel='stylesheet', type='text/css')
    // Theme style
    link(rel='stylesheet', href='css/AdminLTE.css')
    //
      AdminLTE Skins. We have chosen the skin-blue for this starter
      page. However, you can choose any other skin. Make sure you
      apply the skin class to the body tag so the changes take effect.
    link(rel='stylesheet', href='css/skin-green.css')
    // Bootstrap
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', integrity='sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u', crossorigin='anonymous')
    // overrides
    link(rel='stylesheet', href='css/app.css')
    // d3.js
    script(src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.6/d3.min.js", charset="utf-8")
    // radarChart.js
    script(src="js/radarChart.js", type="text/javascript")
  body.hold-transition.skin-green.sidebar-mini
    #app.wrapper
      // Main Header
      include topbar.pug
      // Left side column. contains the logo and sidebar
      include left_sidebar.pug
      // Content Wrapper. Contains page content
      include content.pug
      // /.content-wrapper
      // Main Footer
      include footer.pug